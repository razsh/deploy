#!/usr/bin/env perl

use strict;
use warnings;

use FindBin qw($Bin);
use lib "$Bin/../lib";	

use Error qw(:try);
use Data::Dumper;

use deploy::Config;
use deploy::Git;
use Jira::REST;

use constant NOT_MERGED  => 'Not merged';
use constant MERGED      => 'Merged';
use constant NO_CONFLICT => 'OK';
use constant CONFLICT    => 'Conflicts';

my $release_number = '9.25';
my $release        = 'release' . $release_number;

my $git  = new deploy::Git;
my $jira = new Jira::REST;

my $top_rc_branch = 'BF-24193-Fix-international-homepage-loads.js-rc1';

if ($git->was_merged($release, $top_rc_branch)) {
	print "Yes\n";
} else {
	print "No\n";
}
