#!/usr/bin/env perl

use strict;
use warnings;

use FindBin qw($Bin);
use lib "$Bin/../lib";	

use deploy::Config;
use Jira::REST;
use Data::Dumper;

my $jira = new Jira::REST;
my $project = $jira->get_project(deploy::Config->jira_project_id);

my $ver = (grep($_->{name} =~ /9\.23/, @{ $project->{versions} }))[0];
print Dumper $ver;